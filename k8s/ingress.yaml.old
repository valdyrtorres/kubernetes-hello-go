apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: ingress-host
  annotations:
    kubernetes.io/ingress.class: "nginx"
spec:
  rules:
  - host: "ingress.fullcycle-valdir.com.br"
    http:
      paths:
        - pathType: Prefix
          path: "/"
          backend:
            serviceName: goserver-service
            servicePort: 80